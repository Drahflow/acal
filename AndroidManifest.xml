				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity
			android:label="@string/appActivityMonthView"
			android:name=".activity.MonthView" >
			<intent-filter >
				<action android:name="com.morphoss.acal.activity.MonthView" />
