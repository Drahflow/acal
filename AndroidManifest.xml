			</intent-filter>
		</activity>
		<activity
			android:icon="@drawable/icon"
			android:label="@string/appActivitySettings"
			android:launchMode="standard"
			android:name=".activity.Settings" >
			<intent-filter >
				<action android:name="com.morphoss.acal.activity.Settings" />
