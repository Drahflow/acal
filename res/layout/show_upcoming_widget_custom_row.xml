<?xml version="1.0" encoding="utf-8"?>
<com.morphoss.acal.desktop.ShowUpcomingRowLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="298dip"
	android:layout_height="24dip"
	android:background="@drawable/show_upcoming_widget_row_bg"
	android:orientation="horizontal"
	android:paddingLeft="10dip"
	android:paddingRight="10dip" >
	<TextView
		android:id="@+id/upcoming_date"
		android:layout_width="80dip"
		android:layout_height="23dip"
		android:textSize="15dip" />
	<TextView
		android:id="@+id/upcoming_time"
		android:layout_width="60dip"
		android:layout_height="23dip"
		android:textSize="15dip" />
	<TextView
		android:id="@+id/upcoming_summary"
		android:layout_width="140dip"
		android:layout_height="23dip"
		android:textSize="15dip" />
</com.morphoss.acal.desktop.ShowUpcomingRowLayout>