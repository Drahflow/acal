<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_height="fill_parent" 
  android:layout_width="fill_parent"
  android:padding="0sp" >

	<TextView 
		android:id="@+id/YVmonth_title" 
		android:layout_height="wrap_content" 
		android:layout_width="fill_parent" 
		style="@style/YVmonth_title"
		android:visibility="gone">
	</TextView>
	
	<TextView 
		android:id="@+id/YVyear_title" 
		android:layout_height="wrap_content" 
		android:layout_width="fill_parent"
		style="@style/YVyear_title"
		android:visibility="gone">
	</TextView>
	
	<TextView
		android:id="@+id/YVDayBoxColumnHeader"
		style="@style/YVMonthDayColumnHeader"
		android:layout_height="fill_parent" 
  		android:layout_width="fill_parent" 
  		android:visibility="gone"/>

	<TextView
		android:id="@+id/YVDayBoxInMonth"
		style="@style/YVMonthDayInMonth"
		android:layout_height="fill_parent" 
  		android:layout_width="fill_parent" 
  		android:visibility="gone"/>
		
	<TextView
		android:id="@+id/YVDayShadowed"
		style="@style/YVDayShadowed"
		android:layout_height="fill_parent" 
  		android:layout_width="fill_parent" 
  		android:visibility="gone"/>
  		
</RelativeLayout>
